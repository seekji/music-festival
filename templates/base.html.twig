<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}


    {% block seo_block %}
        {% set seoData = seo_load() %}
        {{ seo_title(seoData) }}
        {{ seo_meta_tags(seoData) }}
    {% endblock %}
</head>
<body>
    {% include 'blocks/header.html.twig' %}

    <main>
      {% block body %}
      {% endblock %}
    </main>

    {% block javascript %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</body>
</html>
